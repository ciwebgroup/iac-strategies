{
  "port": 80,
  "baseURL": "",
  "address": "0.0.0.0",
  "log": "stdout",
  "database": "/database/filebrowser.db",
  "root": "/srv",
  "noAuth": false,
  "signup": false,
  "createUserDir": false,
  "defaults": {
    "scope": "/",
    "locale": "en",
    "viewMode": "list",
    "singleClick": false,
    "sorting": {
      "by": "name",
      "asc": true
    },
    "perm": {
      "admin": false,
      "execute": false,
      "create": true,
      "rename": true,
      "modify": true,
      "delete": true,
      "share": false,
      "download": true
    },
    "commands": [],
    "hideDotfiles": false,
    "dateFormat": true
  },
  "commands": {
    "after_save": [],
    "before_save": [],
    "after_upload": []
  },
  "shell": [],
  "rules": [
    {
      "regex": true,
      "allow": false,
      "regexp": "^\\/\\..*$"
    },
    {
      "regex": true,
      "allow": false,
      "regexp": ".*\\.(exe|sh|bat|cmd|com|pif|scr|vbs|js)$"
    }
  ],
  "branding": {
    "name": "WordPress Farm File Manager",
    "disableExternal": false,
    "files": "",
    "theme": "dark"
  },
  "tus": {
    "chunkSize": 10485760,
    "retryCount": 5
  }
}

